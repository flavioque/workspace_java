
public class isiCarros {
	
	public static void main (String args[]) {
		Carro c1 = new Carro();
		Carro c2 = new Carro();
		
		c1.marca = "Porche";
		c1.modelo = "kaieny";
		c1.cor = "Azul marinho nervoso";
		c1.ano = 2020;
		c1.preco = 125000.00;
		
		c1.marca = "Xevrole";
		c1.modelo = "Corsa";
		c1.cor = "Prata com detalhes brancos";
		c1.ano = 1998;
		c1.preco = 3852.00;
		
		System.out.println("Carro = " + c1.marca + " / " + c1.modelo + " / " + c1.cor + " / " + c1.ano + " / " + c1.preco);
		System.out.println("Carro = " + c2.marca + " / " + c2.modelo + " / " + c2.cor + " / " + c2.ano + " / " + c2.preco);
		
		
	}

}
