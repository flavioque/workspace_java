
public class Teste {
	public static void main (String args[]) {
		
		Empregado e1 = new Empregado();
		Empregado e2 = new Empregado();
		
		e1.nome="Isidro";
		e1.cargo="Professor";
		e1.salario = 10000.00;
		
		e2.nome = "Luana";
		e2.cargo = "Personal Trainer";
		e2.salario = 50000.00;
		
		System.out.println(" =====> Ordenado antes do aumento ...");
		e1.Imprimir();
		e2.Imprimir();
		
		System.out.println(" ====> Aumentando salário");
		e1.aumentarSalario(15.0);
		e2.aumentarSalario(10.0);
	}

}
